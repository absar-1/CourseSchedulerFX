<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.VBox?>

<AnchorPane prefHeight="400.0" prefWidth="600.0" styleClass="anchor-pane"
            xmlns="http://javafx.com/javafx/17"
            xmlns:fx="http://javafx.com/fxml/1"
            fx:controller="com.example.courseschedulerfx.controllers.AdminDashBoardController"
            stylesheets="@../css/admin_dashboard.css">
    <VBox fx:id="mainContainer" id="mainContainer" fillWidth="true"
          AnchorPane.bottomAnchor="0.0"
          AnchorPane.leftAnchor="0.0"
          AnchorPane.rightAnchor="0.0"
          AnchorPane.topAnchor="0.0">
        <fx:include fx:id="header" source="header.fxml" />
    </VBox>
</AnchorPane>
